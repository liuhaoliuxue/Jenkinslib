#!groovy
//引用Library库
@Library('jenkinslib') _  

def build = new org.devops.build()
String buildType = "${env.buildType}" //引用参数
String buildShell = "${env.buildShell}" //引用参数

pipeline {
    agent { 
	    node {
		    label "master" //构建主机
		}
	}
	stages {
	    stage("Build") {
		    steps {
			    script {
				    build.Build(buildType,buildShell)
				}
			}
		}
	}
}
